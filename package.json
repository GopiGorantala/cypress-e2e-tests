{
  "name": "cypress-e2e-tests",
  "version": "1.0.0",
  "description": "Cypress E2E Tests for edX applications",
  "repository": "https://github.com/edx/cypress-e2e-tests",
  "scripts": {
    "cy:verify": "cypress verify",
