    "cypress": "^3.5.0",
    "cypress-failed-log": "^2.5.0",
    "fs-extra": "^8.1.0",
    "googleapis": "42.0.0",
    "uuid": "^3.3.2",
    "webpack": "4.36.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/uuid": "^3.4.5",
    "ts-loader": "5.4.5",
    "typescript": "^3.6.4",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-edx": "^4.0.4",
    "eslint-plugin-cypress": "^2.6.1"
